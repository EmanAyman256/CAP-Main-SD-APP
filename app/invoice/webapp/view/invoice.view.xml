            <table:Column id="_IDGenColumn14" width="120px">
            <table:label><Text id="_IDGenText41" text="Actual Total Percentage %"/></table:label>
                <table:template>
                    <Text id="_IDGenText16" text="{UnlimitedOverFulfillment}"/>
                </table:template>
            </table:Column>

            <!-- <table:Column id="_IDGenColumn15" width="120px">
                <table:label><Text id="_IDGenText42" text="Manual Price Entery Allowd"/></table:label>    
