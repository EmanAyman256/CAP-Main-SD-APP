<mvc:View controllerName="invoice.controller.View1"
    xmlns:table="sap.ui.table"
    xmlns:layout="sap.ui.layout"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:r="sap.ui.table.rowmodes"
    xmlns="sap.m">
    <Page id="page2" title="{i18n>title}">
     <Panel id="_IDGenPanel2" backgroundDesign="Solid">
    <VBox id="_IDGenVBox" >
            <HBox id="_IDGenHBox" justifyContent="SpaceBetween" alignItems="Center">
                <Text id="_IDGenText28" text="Document: 20000010 " />
                <HBox id="_IDGenHBox1">
                    <Button id="_IDGenButton2" text="Save Document" icon="sap-icon://save" type="Transparent" press=".onSaveDocument"/>
                    <Button id="_IDGenButton3" text="Print" icon="sap-icon://print" type="Transparent" press=".onPrint"/>
                    <Button id="_IDGenButton4" text="Export" icon="sap-icon://download" type="Transparent" press=".onExport"/>
                    <Button id="_IDGenButton5" text="Import" icon="sap-icon://upload" type="Transparent" press=".onImport"/>
                    <Button id="_IDGenButton6" text="Delete" icon="sap-icon://delete" type="Transparent" press=".onDelete"/>
                </HBox>
            </HBox>
