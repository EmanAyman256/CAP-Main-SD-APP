            <Label id="_IDGenLabel1" class="myLabel sapUiSmallMargin" design="Bold" />
            <Button id="_IDGenButton2" class="sapUiSmallMargin" type= "Emphasized" text="Save Document" icon="sap-icon://save" press="onSaveDocument"/>
            <Button id="_IDGenButton3" class="sapUiSmallMargin" type= "Emphasized" text="Print"  icon="sap-icon://print" press="onPrint"/>
            <Button id="_IDGenButton4" class="sapUiSmallMargin" type= "Emphasized" text="Export" icon="sap-icon://arrow-bottom" press="onExport"/>
            <Button id="_IDGenButton5" class="sapUiSmallMargin" type= "Emphasized" text="Import" icon="sap-icon://arrow-top" press="onImport"/>
        </HBox> -->
        <HBox id="_IDGenHBox2" class="sapUiSmallMargin" justifyContent="Center" alignItems="Center">
            <VBox id="_IDGenVBox4" class="myRectangle" alignItems="Center" justifyContent="Center">
                <Text id="_IDGenText44" text="Total Value:" />
            </VBox>
            <VBox id="_IDGenVBox5" class="myRectangle" alignItems="Center" justifyContent="Center">
                <Text id="_IDGenText1" />
            </VBox>
            <VBox id="_IDGenVBox6" class="myRectangle" alignItems="Center" justifyContent="Center">
                <Text id="_IDGenText2" text="SAR"  />
            </VBox>
        </HBox>
    </VBox>
    </Panel>

    <HBox id="_IDGenVBox7" justifyContent="SpaceBetween" class="sapUiSmallMargin">
        <SearchField id="_IDGenSearchField" ></SearchField>
        <Button id="_IDGenButton1" icon="sap-icon://delete" press="onDeleteItem"/>
    </HBox>

    <table:Table id="_IDGenTable" selectionMode= "MultiToggle"   rows="{/Items}" enableColumnReordering="false">
        
        <table:rowMode>
            <r:Auto id="_IDGenFixed" minRowCount="5" maxRowCount="10" rowContentHeight="44" />
        </table:rowMode>

        <table:columns>

            <table:Column id="_IDGenColumn1" >
                <table:label><Text id="_IDGenText3" text="Print"/></table:label>
                <table:template>
                    <CheckBox id="_IDGenCheckBox" text="{Checked}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn2" width="120px">
                <table:label><Text id="_IDGenText4" text="Main Item No."/></table:label>
                <table:template>
                    <Text id="_IDGenText30" text="{MainItemNo}" />
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn3" width="120px">
                <table:label><Text id="_IDGenText5" text="Line.NR"/></table:label>
                <table:template>
                    <Text id="_IDGenText31" text="{LineNR}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn4" width="120px">
                <table:label><Text id="_IDGenText32" text="Service No."/></table:label>
                <table:template>
                    <Text id="_IDGenText6" text="{ServiceNo}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn5" width="120px">
                <table:label><Text id="_IDGenText33" text="Description"/></table:label>
                <table:template>
                    <Text id="_IDGenText7" text="{Description}"/>
                </table:template>
            </table:Column>

          

            <table:Column id="_IDGenColumn7" width="120px">
                <table:label><Text id="_IDGenText35" text="UOM"/></table:label>
                <table:template>
                    <Text id="_IDGenText9" text="{UOM}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn8" width="120px">
                <table:label><Text id="_IDGenText36" text="Amount Per Unit"/></table:label>
                <table:template>
                    <Text id="_IDGenText10" text="{AmountPerUnit}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn9" width="120px">
                <table:label><Text id="_IDGenText37" text="Currency"/></table:label>
                <table:template>
                    <Text id="_IDGenText11" text="{Currency}"/>
                </table:template>
            </table:Column>

              <table:Column id="_IDGenColumn6" width="120px">
                <table:label><Text id="_IDGenText34" text="Current Amount"/></table:label>
                <table:template>
                    <Text id="_IDGenText8" text="{QTY}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn10" width="120px">
            <table:label><Text id="_IDGenText38" text="Total Quantity"/></table:label>
                <table:template>
                    <Text id="_IDGenText12" text="{Total}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn11" width="120px">
            <table:label><Text id="_IDGenText39" text="Current Quantity"/></table:label>
                <table:template>
                    <Text id="_IDGenText13" text="{ActualQTY}"/>
                </table:template>
            </table:Column>
            <table:Column id="_IDGenColumn27" width="120px">
            <table:label><Text id="_IDGenText55" text="Current Percentage"/></table:label>
                <table:template>
                    <Text id="_IDGenText56" text="{ActualQTY}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn12" width="120px">
            <table:label><Text id="_IDGenText40" text="Remaining Quantity"/></table:label>
                <table:template>
                    <Text id="_IDGenText14" text="{ActualPercentage}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn13" width="120px">
                <table:label><Text id="_IDGenText29" text="Actual Total Quantity"/></table:label>
                <table:template>
