                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn19" width="120px" >
                <table:label><Text id="_IDGenText47" text="Service Text"/></table:label> 
                <table:template>
                     <Text id="_IDGenText21" text="{ServiceText}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn20" width="120px">
                <table:label><Text id="_IDGenText48" text="Line Text"/></table:label> 
                <table:template>
                    <Text id="_IDGenText22" text="{LineText}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn21" width="120px">
                <table:label><Text id="_IDGenText49" text="Personnel.NR"/></table:label> 
                <table:template>
                    <Text id="_IDGenText23" text="{PersonnelNR}"/>
                </table:template>
            </table:Column>

            <table:Column id="_IDGenColumn22" width="120px">
                <table:label><Text id="_IDGenText50" text="Line Type"/></table:label> 
                <table:template>
                    <Text id="_IDGenText24" text="{LineType}"/>
                </table:template>
                
            </table:Column>

            <table:Column id="_IDGenColumn23" width="120px">
                <table:label><Text id="_IDGenText51" text="Bidders'line"/></table:label> 
                <table:template>
                    <Text id="_IDGenText25" text="{Biddersline}"/>
                </table:template>
                
            </table:Column>

            <table:Column id="_IDGenColumn24" width="120px">
