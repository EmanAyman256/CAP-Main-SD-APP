    <Page id="_IDGenPage2" title="Service Types">
          <Table id="serviceTable" items="{/ServiceTypes}">
            <columns>
                <Column id="_IDGenColumn1">
                    <Text id="_IDGenText" text="Code" />
                </Column>
                <Column id="_IDGenColumn2">
                    <Text id="_IDGenText1" text="Description" />
                </Column>
                <Column id="_IDGenColumn3">
                    <Text id="_IDGenText2" text="Created On" />
                </Column>
                <Column id="_IDGenColumn4">
                    <Text id="_IDGenText3" text="Actions" />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem">
                    <cells>
                        <Text id="_IDGenText4" text="{Code}" />
                        <Text id="_IDGenText5" text="{Description}" />
                        <Text id="_IDGenText6" text="{CreatedOn}" />
                        <HBox id="_IDGenHBox">
                            <Button id="_IDGenButton1" icon="sap-icon://edit" press="onEdit" />
                            <Button id="_IDGenButton2" icon="sap-icon://delete" press="onDelete" />
                        </HBox>
                    </cells>
                </ColumnListItem>
            </items>
